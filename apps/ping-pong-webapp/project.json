{
  "name": "ping-pong-webapp",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/ping-pong-webapp/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nrwl/vite:build",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "production",
      "options": {
        "outputPath": "dist/apps/ping-pong-webapp"
      },
      "configurations": {
        "development": {
          "mode": "development"
        },
        "production": {
          "mode": "production"
        }
      }
    },
    "serve": {
      "executor": "@nrwl/vite:dev-server",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "ping-pong-webapp:build"
      },
      "configurations": {
        "development": {
          "buildTarget": "ping-pong-webapp:build:development",
          "hmr": true
        },
        "production": {
          "buildTarget": "ping-pong-webapp:build:production",
          "hmr": false
        }
      }
    },
    "preview": {
      "executor": "@nrwl/vite:preview-server",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "ping-pong-webapp:build"
      },
      "configurations": {
        "development": {
          "buildTarget": "ping-pong-webapp:build:development"
        },
        "production": {
          "buildTarget": "ping-pong-webapp:build:production"
        }
      }
    },
    "test": {
      "executor": "@nrwl/vite:test",
      "outputs": ["coverage/apps/ping-pong-webapp"],
      "options": {
        "passWithNoTests": true,
        "reportsDirectory": "../../coverage/apps/ping-pong-webapp"
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["apps/ping-pong-webapp/**/*.{ts,tsx,js,jsx}"]
      }
    }
  },
  "tags": []
}
